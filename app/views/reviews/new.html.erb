<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h1>Add a new review</h1>
      <%= simple_form_for [@restaurant, @review] do |f| %>
        <%= f.input :content %>
        <%= f.input :rating %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
